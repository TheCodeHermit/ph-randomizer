area RuinsIsle:
  room BottomLeft:
    node Boat:
      door Boat
      door ToCaveBoat

    node AboveMaze:
      door ToCaveUpper
      enemy RupeeLikeRed1
      enemy RupeeLikeRed2
      enemy RupeeLikeRed3
      enemy RupeeLikeBlue1
      enemy RupeeLikeBlue2
      door ToNorthRight

    AboveMaze -> Boat


    node AboveCave:
      door ToNorthShortcut
      door ToEastShortcut

    AboveCave -> Boat
    AboveCave -> AboveMaze

    node Underwater:
      door ToNorthUnderwater
      enemy RupeeLikeGreen1
      enemy RupeeLikeGreen2
      enemy RupeeLikeRed

    node ToNorthLeft:
      door ToNorthLeft

    AboveMaze -> Underwater: flag KingKeyInserted
    ToNorthLeft <-> Underwater: flag KingKeyInserted


  room TopLeft:
    node FromSouthRight:
      door ToSouthRight

    node FromSouthLeft:
      door ToSouthLeft

    node Main:
      enemy TreeStalfos1
      enemy TreeStalfos2
      enemy RupeeLikeRed
      door ToBremeurPyramid

    FromSouthRight -> Main
    FromSouthLeft -> Main


    node AfterBremeurBridge:
      enemy PirateBlin1
      enemy PirateBlin2
      enemy PirateBlin3
      enemy PirateBlin4
      enemy PirateBlin5
      door ToEastBottom

    Main -> AfterBremeurBridge: flag BremeurBridge
    AfterBremeurBridge -> Main


    node ToSouth:
      enemy Stalfos
      door ToSouthShortcut

    Main -> ToSouth
    AfterBremeurBridge -> ToSouth


    node FromEast:
      door ToEastTop
      enemy PirateBlin1
      enemy PirateBlin2
      enemy PirateBlin3

    FromEast -> Main


    node ToSouthUnderwater:
      chest Underwater # power gem
      door ToSouthUnderwater

    ToSouth -> ToSouthUnderwater: flag KingKeyInserted


    node NearPyramidUnderwater:
      enemy Stalfos1
      enemy Stalfos2
      door ToEastUnderwaterTop

    node AfterBremeurUnderwater:
      door ToEastUnderwaterBottom

    Main -> NearPyramidUnderwater: flag KingKeyInserted
    AfterBremeurBridge -> AfterBremeurUnderwater: flag KingKeyInserted


  room TopRight:
